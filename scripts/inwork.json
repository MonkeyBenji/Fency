        "version": "0.1",
        "name": "Search Phone Number",
        "description": "Voegt context menu toe om 06 nummer te Googelen van persoontje",
        "matches": "<all_urls>",
        "path": "misc/phone-search.js",
        "enabled": true,
        "allFrames": false
      },
      {
        "id": "intranet-links",
        "type": "script",
        "version": "0.1",
        "name": "Intranet Linkjes",
        "description": "Maakt het wat duidelijker dat die intranet linkjes linkjes zijn",
        "matches": "https://intranet.inwork.nl/",
        "path": "misc/intranet-links.js",
        "enabled": true,
        "allFrames": false
      },
      {
        "id": "april-fools",
        "type": "script",
        "version": "0.1",
        "name": "Fake News",
        "description": "April 1st, frog in your buttock!",
        "matches": [
          "https://nos.nl/*",
          "https://www.nu.nl/*",
          "https://inwork.nl/*"
        ],
        "path": "misc/april-fools.js",
        "enabled": true,
        "allFrames": false
      }
    ]
  }
}
