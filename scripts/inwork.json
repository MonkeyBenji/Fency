{
  "version": "0.0.0.1",
  "scripts": {
    "InWork": [
      {
        "id": "vaca-shortcuts",
        "type": "script",
        "version": "0.1",
        "name": "InWork Vacature Connexys Link",
        "description": "Maakt van vacature titels hieperlinks naar het Connexys object",
        "matches": [
          "https://inwork.nl/*",
          "https://www.inwork.nl/*",
          "https://www-dev.inwork.nl/*"
        ],
        "path": "inwork/vaca-shortcuts.js",
        "enabled": true
      }
    ],
    "Connexys": [
      {
        "id": "textarea-autobullets",
        "type": "script",
        "version": "0.1",
        "name": "TextArea autobullets",
        "description": "Zet automagisch een â€¢ en wat bijzondere spaties aan het begin van de regel als je vorige regel eindigde met een : en iets met ; en zo",
        "matches": "https://*.lightning.force.com/*",
        "path": "connexys/textarea-autobullets.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "auto-login-365",
        "type": "script",
        "version": "0.1",
        "name": "Office 365 auto login",
        "description": "Klikt meteen op de Office 365 inlog knop, nog voordat je kan besluiten om op de verkeerde manier in te loggen",
        "matches": "https://inwork.cloudforce.com/",
        "path": "connexys/auto-login-365.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "remove-padding",
        "type": "script",
        "version": "0.1",
        "name": "Remove Padding",
        "description": "Voegt context menu toe om padding uit een textarea weg te bonjouren",
        "matches": "<all_urls>",
        "path": "connexys/remove-padding.js",
        "enabled": true,
        "allFrames": false
      },
      {
        "id": "geo-col",
        "type": "script",
        "version": "0.1",
        "name": "Geo Coordinaten Kolom",
        "description": "Snelle methode om in een Salesforce rapport een kolom toe te voegen met de afstand tot een bepaalde locatie",
        "matches": "https://inwork.lightning.force.com/reports/lightningReportApp.app*",
        "path": "connexys/geo-col.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "vaca-font",
        "type": "script",
        "version": "0.1",
        "name": "Vaca font fixer",
        "description": "Zorgt dat je vacature niet vernoikt wordt als je van Word naar Knexis kopie pasta'd",
        "matches": "https://inwork.lightning.force.com/*",
        "path": "connexys/vaca-font.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "remove-default-filters",
        "type": "script",
        "version": "0.1",
        "name": "#nofilter",
        "description": "Verwijder de standaard filters van een Salesforce rapport met 1 klik",
        "matches": "https://inwork.lightning.force.com/reports/lightningReportApp.app*",
        "path": "connexys/remove-default-filters.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "anonimize-default-state",
        "type": "script",
        "version": "0.1",
        "name": "Status na anonimiseren",
        "description": "Zet de standaard status bij anonimiseren op 9.0, omdat de standaard standaard (6.0 aangenomen) niet heel logisch is...",
        "matches": "https://*.visual.force.com/apex/cxsAnonymize*",
        "path": "connexys/anonimize-default-state.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "dash-autorefresh",
        "type": "script",
        "version": "0.1",
        "name": "Dashboard Auto-refresh",
        "description": "Automatically refresh dashboards when Salesforce warns they're old",
        "matches": ["https://inwork.lightning.force.com/desktopDashboards/*"],
        "path": "connexys/dash-autorefresh.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "matching-customers",
        "type": "script",
        "version": "0.1",
        "name": "Matchende klanten zoekert",
        "description": "Maak vanuit een kandidaat, automagisch een rapport van klanten op basis van Geo/BU/Functie",
        "matches": "https://inwork.lightning.force.com/*",
        "path": "connexys/matching-customers.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "add-to-vacancy",
        "type": "script",
        "version": "0.1",
        "name": "Snellere recr. inschrijving",
        "description": "Voeg een kandidaat toe aan je recente vacature vanuit een FAB menu'tje bij de kandidaat",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/add-to-vacancy.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "cv-generator",
        "type": "script",
        "version": "0.1",
        "name": "CV Generator Fixes",
        "description": "Kut Connexys updates ook (tekst in datum veld, auto anoniem, styling, etc.)",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/cv-generator.js",
        "enabled": true
      },
      {
        "id": "cv-docx",
        "type": "script",
        "version": "0.1",
        "name": "CV Word (beta)",
        "description": "Word CV download knop ding",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/cv-docx.js",
        "enabled": true
      },
      {
        "id": "step-style",
        "type": "script",
        "version": "0.1",
        "name": "Step styling",
        "description": "Zet wat veldjes naast elkaar voor betere UI",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/step-style.js",
        "enabled": true
      },
      {
        "id": "new-candidate-style",
        "type": "script",
        "version": "0.1",
        "name": "Nieuwe kandidaat optimalisaties",
        "description": "Zet wat veldjes naast elkaar voor betere UI; Kopie pasta functionaliteit InWork.club mail",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/new-candidate-style.js",
        "enabled": true
      },
      {
        "id": "default-report-filter-iw",
        "type": "script",
        "version": "0.1",
        "name": "Standaard [iw] filter",
        "description": "Standaard [iw] filter bij nieuw rapport",
        "matches": [
          "https://inwork.lightning.force.com/reports/lightningReportApp.app"
        ],
        "path": "connexys/default-report-filter-iw.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "default-subject-empty",
        "type": "script",
        "version": "0.1",
        "name": "Standaard leeg onderwerp taak",
        "description": "Is nu standaard \"Bellen\" ... ... teh fuk Salesforce",
        "matches": ["https://inwork.lightning.force.com/*"],
        "path": "connexys/default-subject-empty.js",
        "enabled": true,
        "allFrames": true
      },
      {
        "id": "logo",
        "type": "script",
        "version": "0.1",
        "name": "Clickable Connexys Logo",
        "description": "Anders weet ik mijn weg terug niet als ik in de Admin verdwaal",
        "matches": [
          "https://*.lightning.force.com/*",
          "https://*.cloudforce.com/*"
        ],
        "path": "connexys/logo.js",
        "enabled": true
      }
    ],
    "Misc": [
      {
        "id": "directlease-contract-kevin",
        "type": "script",
        "version": "0.1",
        "name": "DirectLease Contract Kevin",
        "description": "Set contract settings to Kevin preferences automagically",
        "matches": "https://directlease.nl/leasen/*",
        "path": "misc/directlease-contract-kevin.js",
        "enabled": true
      },
      {
        "id": "phone-search",
