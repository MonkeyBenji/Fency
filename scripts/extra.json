{
  "version": "0.0.0.1",
  "scripts": {
    "InWork": [
      {
        "id": "inwork-bananen",
        "type": "script",
        "version": "0.1",
        "name": "Groene bananen",
        "description": "Banaan!",
        "matches": ["https://inwork.nl/*", "https://www.inwork.nl/*"],
        "path": "inwork/bananen.js",
        "enabled": false
      }
    ],
    "InCTRL": [
      {
        "id": "auto-mike",
        "type": "script",
        "version": "0.1",
        "name": "AutoMike",
        "description": "Redirect InCTRL to mike.inwork.nl",
        "matches": "https://inctrl.inwork.nl/*",
        "path": "inctrl/auto-mike.js",
        "enabled": false
      }
    ],
    "Misc": [
      {
        "id": "directlease-contract-kevin",
        "type": "script",
        "version": "0.1",
        "name": "DirectLease Contract Kevin",
        "description": "Set contract settings to Kevin preferences automagically",
        "matches": "https://directlease.nl/leasen/*",
        "path": "misc/directlease-contract-kevin.js",
        "enabled": false
      }
    ]
  }
